{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-param-reassign": "off",
    "no-use-before-define": "off",
    "prettier/prettier": "error",
    "ignoreExternal": true,
    "import/prefer-default-export": "off"
  }
}
